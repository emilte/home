self="~/.bash_utils"
echo
echo "=== $self ==="

function vsource {
	# Verbose source of path if it exists.
	# Usage: vsource <PATH> <FROM>
	# Example: vsource ~/.bashrc "here"
	arg_path=$1
	arg_from=$2 ; arg_from=${arg_from:=`pwd`} # Set default parameter if missing.
	[ -f $arg_path ] && echo "Sourcing $arg_path" && . $arg_path
}